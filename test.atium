export func factorial (count: i32) -> i32 {
	let result: i32 = 1;
	let i: i32 = 0
	while i <= count {
			i = i + 1
			result = result * i
	}
}

export func fibonacci(n: i32) -> i32 {
	if n <= 1 {
		n
	}
	else {
		fibonacci(n - 2) + fibonacci(n - 1)
	}
}

func point (x: i32, y: i32) -> (i32, i32) {
	(x, y)
}

export func pointA(x: i32, y: i32) -> i32 {
	let a: (i32, i32) = point(x, y);
	a.0
}

export func pointB(x: i32, y: i32) -> i32 {
	let a = point(x, y);
	a.1
}